{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/coreui-free-angular-admin-template",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "functions"
  },
  "extensions": {}
}
